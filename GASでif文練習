function dailyLogUpdate() {
  // 1. シートの取得
  const sourceSheet = SpreadsheetApp.openById('AS回線スプレッドシートのID').getSheetByName('シート名');
  const logSheet = SpreadsheetApp.openById('貼り付け先のスプレッドシートID').getSheetByName('ADM_N発回線');
  
  // 2. データの取得
  const headers = sourceSheet.getRange('3:3').getValues()[0];  // ヘッダー行を3行目に変更
  // 改行を含む列名で検索
  const phaseCol = headers.findIndex(header => header.toString().includes('撤去') && header.toString().includes('nPhase')) + 1;
  
  if (phaseCol === 0) {
    throw new Error('列「撤去/nPhase」が見つかりません');
  }
  
  // データ範囲を取得（4行目から最終行まで）
  const lastRow = sourceSheet.getLastRow();
  const phaseData = sourceSheet.getRange(4, phaseCol, lastRow - 3, 1).getValues();  // 開始行を4行目に変更
  
  const todayData = phaseData.map(row => [
    row[0] === '' ? '' : row[0]
  ]);
  
  // 3. データの貼り付け
  const date = Utilities.formatDate(new Date(), 'Asia/Tokyo', 'yyyy/MM/dd');
  logSheet.insertColumnsAfter(1, 1);
  
  // 日付を1行目に設定
  logSheet.getRange(1, 2).setValue(date);
  // ヘッダーを3行目に設定（改行を含む形式で設定）
  logSheet.getRange(3, 2).setValue("撤去\nnPhase");  // 改行を \n で表現
  // データを4行目から貼り付け
  logSheet.getRange(4, 2, todayData.length, 1).setValues(todayData);
  
  // 4. 前日データと比較して差分を通知
  if (logSheet.getLastColumn() >= 3) {
    const yesterdayData = logSheet.getRange(4, 3, todayData.length, 1).getValues();
    let message = '';
    
    for (let i = 0; i < todayData.length; i++) {
      if (todayData[i][0] !== yesterdayData[i][0]) {
        message += `${i + 4}行目:\n`;
        message += `撤去/nPhase: ${yesterdayData[i][0] || '空白'} → ${todayData[i][0] || '空白'}\n`;
      }
    }
    
    if (message) {
      const webhook = "https://chat.googleapis.com/v1/spaces/AAAAWiNFgzw/messages?key=AIzaSyDdl0hCZlE6yvSjMm-WEfRq3CPzqKqqsHI&token=Md4sZXHc5KO8-XFC95YRZZhT9wtoRCXFb46zSY";
      UrlFetchApp.fetch(webhook, {
        method: 'post',
        contentType: 'application/json',
        payload: JSON.stringify({ text: date + "の差分:\n" + message })
      });
    }
  }
}
